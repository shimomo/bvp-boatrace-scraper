{
    "name": "bvp/boatrace-scraper",
    "description": "The BVP Boatrace Scraper package for Boatrace.",
    "keywords": [
        "boatrace",
        "library",
        "scraper"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "shimomo",
            "email": "bvp@shimomo.net"
        }
    ],
    "require": {
        "php": "^8.2",
        "bvp/converter": "^4.0",
        "bvp/trimmer": "^4.0",
        "nesbot/carbon": "^2.63 || ^3.0",
        "symfony/browser-kit": "^6.0 || ^7.0",
        "symfony/css-selector": "^6.0 || ^7.0",
        "symfony/http-client": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1 || ^11.1 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "BVP\\BoatraceScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BVP\\BoatraceScraper\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
